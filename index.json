{
	"checkversion":true,
	"showanimate":true,
	"seojsontpl":"-seo/?path={crumb.query}",
	"data":true,
	"tpl":"-bugagashop/index.tpl",
	"divs":{
		"BODY":{
			"json":"-excel/get/group/Главная/?src=~auto/pages/Параметры.xlsx",
			"dataroot":"data.data.descr",
			"tpl":"-bugagashop/layout.tpl",
			"tplroot":"BODY",
			"divs":{
				"USERMENU1":{
					"tpl":"-bugagashop/layout.tpl",
					"activelink":true,
					"onlyclient":true,
					"tplroot":"USERMENU1",
					"global":["user","cart"],
					"json":"-cart/?type=order&id="
				},
				"USERMENU2":{
					"tpl":"-bugagashop/layout.tpl",
					"activelink":true,
					"onlyclient":true,
					"tplroot":"USERMENU2",
					"global":["user","cart"],
					"json":"-cart/?type=order&id="
				},
				"SEARCHFORM":{
					"tplroot":"SEARCHFORM",
					"tpl":"-bugagashop/layout.tpl",
					"autosavename":"catalog"
				},
				"SUBSCRIBE":{
					"json":"-excel/get/group/Главная/?src=~auto/pages/Параметры.xlsx",
					"dataroot":"data.data.descr",
					"tplroot":"SUBSCRIBE",
					"tpl":"-bugagashop/layout.tpl",
					"goal":"subscribe",
					"autosavename":"user",
					"onsubmit":true
				},
				"COUNTER":{
					"tpl":"~counters.tpl"
				},
				"FOOTGROUPS":{
					"tplroot":"FOOTGROUPS",
					"tpl":"-bugagashop/layout.tpl",
					"json":"-showcase/api/groups"
				},
				"FOOTINFO":{
					"tplroot":"FOOTMENU",
					"tpl":"-bugagashop/layout.tpl",
					"json":"-excel/get/group/Информация/?src=~auto/pages/Параметры.xlsx"
				},
				"FOOTSERVICE":{
					"tplroot":"FOOTMENU",
					"tpl":"-bugagashop/layout.tpl",
					"json":"-excel/get/group/Услуги/?src=~auto/pages/Параметры.xlsx"
				},
				"MAINMENU1":{
					"json":"-excel/get/group/Главное%20меню/?src=~auto/pages/Параметры.xlsx",
					"tpl":"-bugagashop/layout.tpl",
					"tplroot":"MAINMENU"
				},
				"MAINMENU2":{
					"json":"-excel/get/group/Главное%20меню/?src=~auto/pages/Параметры.xlsx",
					"tpl":"-bugagashop/layout.tpl",
					"tplroot":"MAINMENU"
				},
				"CONTENT":[{
					"external":"-bugagashop/index-main.json"					
				},{
					"deep":1,
					"external":"-bugagashop/index-page.json"
				}]
			}
		}
	}
}