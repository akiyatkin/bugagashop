{
	"layers":[{
		"parsedtpl":"{crumb.child.name=:catalog?(crumb.child.child.child??:yes)}",
		"tpl":"-bugagashop/layout.tpl",
		"tplroot":"PAGE",
		"divs":{
			"MAINFILTERS":{
				"istpl2":"{crumb.child.name=:catalog?:yes}",
				"divcheck":true,
				"showanimate":false,
				"jsontpl":"-showcase/api/filters?val={crumb.child=:catalog?~encode(crumb.child.child.name)}&m={~encode(Crumb.get.m)}",
				"tpl":"-catalog/filters.tpl",
				"onlyclient":false
			},
			"BIGACTION":{
				"divcheck":true,
				"showanimate":false,
				"parsedtpl":"{Crumb.get.m}",
				"json":"-excel/get/group/Главная/?src=~auto/pages/Параметры.xlsx",
				"dataroot":"data.data.descr",
				"tpl":"-bugagashop/layout.tpl",
				"tplroot":"BIGACTION"
			},
			"GROUPS":{
				"istpl2":"{crumb.child.name=:catalog?:yes}",
				"external":"-bugagashop/groups.layer.json"
			},
			"PAGE":[{
				"external":"-rubrics/rubrics.layer.json"
			},{
				"childs":{
					"bonus":{
						"data":true,
						"autosavename":"orders.my",
						"tpltpl":"-rubrics/?src=~auto/pages/&id={crumb.name}"
					},
					"user":{
						"external":"-user/user.layer.json"
					},
					"cart":{
						"external":"-cart/cart.layer.json"
					},
					"catalog":[{
						"external":"-catalog/catalog.layer.json"
					}],
					"form":{
						"tplroot":"page",
						"data":true,
						"external":"-contacts/contacts.layer.json"
					}
				}
			}]
		}
	}]
}